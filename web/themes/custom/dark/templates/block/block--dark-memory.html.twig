<div{{ attributes }}>
  {{ title_prefix }}
  {% if label %}
    <h3{{ title_attributes }}>{{ label }}</h3>
  {% endif %}
  {{ title_suffix }}
  
  {% block content %}
  
    {# Exclure les termes de taxonomie du contenu #}
    {% set content_without_taxonomy = content|without('field_projectsskills') %}
    
    <div class="imgtxt">
      {{ content_without_taxonomy }}
    </div>

    <div>
        {{ content.field_projectsskills }}
    </div>
  {% endblock %}
</div>
